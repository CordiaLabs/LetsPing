{
    "name": "@letsping/sdk",
    "version": "0.1.1",
    "description": "The Human-in-the-Loop SDK for Autonomous Agents",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
